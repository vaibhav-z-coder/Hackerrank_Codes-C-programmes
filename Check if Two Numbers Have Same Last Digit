#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
  int num1;
  int num2;
  scanf("%d", &num1);
  scanf("%d", &num2);
  int res1=num1-(num1/10)*10;
  int res2=num2-(num2/10)*10;
  
  if(res1==res2){
    printf("YES");
  }
  else{
    printf("NO");
  }
  
  
  

    return 0;
}
